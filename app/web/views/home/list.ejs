<div class="row gx-30px gy-5">
  <div class="col">
    <div class="box">
      <div class="box-header">
        <h2 class="box-title">Danh sách khám <small class="text-icon fw-6">(<%=countArticle%>)</small>
        </h2>
        <a class="box-more-link link-primary ms-auto" href="/examine">
          Xem đầy đủ
          <svg class="iconsvg-arrow-right ms-1 flex-shrink-0">
            <use xlink:href="/public/content/images/sprite.svg#arrow-right"></use>
          </svg>
        </a>
      </div>
      <div class="box-body box-table">
        <div class="table-responsive-md table-responsive-flush">
          <div class="table-responsive-inner">
            <table class="table-1 table table-pe-x-3 mb-0 align-middle">
              <thead>
                <tr>
                  <td>Id</td>
                  <td>Tên bệnh nhân</td>
                  <td>Phone</td>
                  <td>Address</td>
                  <td>Chẩn đoán</td>
                  <td width="100px">Thời gian</td>
                  <td class="w-1">Trạng thái</td>
                </tr>
              </thead>
              <tbody>
              <% if (listArticle.length > 0){%>
                <% for (i = 0;i < listArticle.length;i++){%>
                  <tr>
                    <td class="fw-6 text-body-2"><%=(i+1)%></td>
                    <td>
                      <a target="_blank" href="/examine/edit/<%=listArticle[i].book_id%>">
                        <span class="text-primary fw-6 fs-13px">
                          <%=listArticle[i].id_request%>
                        </span>
                      </a>
                    </td>
                    <td>
                      <p class="fw-14px mb-1"><%=listArticle[i].title_send_request%></p>
                      <div class="fs-13px">
                        <span class="text-gray-4"><%=listArticle[i].domain%></span>
                        <% if (listArticle[i].channel_name){%>
                        <span class="text-gray-2 mx-1">|</span>
                        <span class="text-red"><%=listArticle[i].channel_name%></span>
                        <%}%>
                        <% if (listArticle[i].fm_name){%>
                        <span class="text-gray-2 mx-1">|</span>
                        <span class="text-primary"><%=listArticle[i].fm_name%></span>
                        <%}%>
                      </div>
                    </td>
                    <td class="fs-13px"><%=moment(listArticle[i].created_at).format("HH:mm:ss")%>
                      <br>
                      <small class="text-body-2"><%=moment(listArticle[i].created_at).format("DD/MM/YYYY")%></small>
                    </td>
                    <td>
                      <span class="badge badge-<%=webService.bookingStatusClass(listArticle[i].search_status).name%>">
                        <%=webService.bookingStatusClass(listArticle[i].search_status).value%>
                      </span>
                    </td>
                  </tr>
                <%}%>
              <%} else {%>
                <tr>
                  <td colspan="5" style="text-align: center;">
                    <b>Không có dữ liệu!</b>
                  </td>
                </tr>
              <% } %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>